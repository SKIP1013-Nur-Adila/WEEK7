package week7;
import java.util.Scanner;
public class Exercise4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number =(int)(Math.random()*11);
		Scanner input=new Scanner(System.in);
		
		int j , i=-1;
		System.out.println("Ini adalah sistem meneka nombor ");
	
		
		while(i !=number){
			System.out.println("Masukkan nombor antara 0-10");
			j=input.nextInt();
			
			if (j==number) {
				System.out.println("Tahniah !! No Anda Sama");
				
			}else if (j <= number) {
				System.out.println("Maaf !! Nombor anda terlalu rendah");
		} else{
			System.out.println("Maaf !! Nombor anda terlalu besar");
		}
			
		}
			
			
			
		}

	}


