package week7;

public class Exercise1 {
	public static void main(String[] args) {
		
//declaration	
	int i=1;
	int j=1;
	int k;
	
	
	while (i<5){
	      System.out.println("i= "+ i);
	      
	      i=i+1;   //i++
	        } 
//	
	do {
		System.out.println("j= "+ j);
	      
	      j=j+1;
	}while (j<5);
	
//
	for(k=1; k<5 ;k++) {
		System.out.println("k= "+k);
	}
		
	
	
	

}}
